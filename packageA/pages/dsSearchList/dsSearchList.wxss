/* packageA/pages/dsSearchList/dsSearchList.wxss */
@import "../../../iconfont.wxss";
page{
  width: 100%;
  height: 100%;
}
#header input{
  width: 70vw;
  height: 64rpx;
  background:rgba(238,238,238,1);
  border-radius:32rpx;
  border: none;
  outline: none;
  margin: 0 3%;
  padding-left: 68rpx;
  background-image:url('https://testhdt.teadrf.cn/wxbackground/search_icon.png');
  background-repeat: no-repeat;
  background-size: 32rpx 32rpx;
  background-position: 24rpx center;
}
.storeImg{
  width: 30vw;
  height: 30vw;
  min-width: 30vw;
  position: relative;
}
.storeImg image{
  width: 30vw;
  min-width: 30vw;
  position: relative;
}
