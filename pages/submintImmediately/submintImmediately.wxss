page{
  padding-bottom: 85rpx;
}
.orderInfoIcon{
  width: 24rpx;
  height: 24rpx;
}
.phoneIcon{
  width: 34rpx;
  height: 40rpx;
}
.cont5{
  margin-bottom: 96rpx;
}
.cont5 textarea{
  width: calc( 100% - 48rpx );
  padding: 24rpx;
  height: 151rpx;
  outline: none;
}

.footer{
  width: 100%;
  height: 85rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
}
.footer text{
  width:33.33%;
  height:85rpx;
  line-height: 85rpx;
  text-align: center;
  color: #fff;
  font-weight: bold;
  background:rgba(255,163,3,1);
  box-shadow:0px 0px 30rpx 0px rgba(224,224,224,1);
  position: absolute;
  right: 0;
}
