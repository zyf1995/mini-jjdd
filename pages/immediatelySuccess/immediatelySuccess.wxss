@import "../../iconfont.wxss";
.nav {
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
  background: #ff8339;
  }
   
  .title_text {
  width: 100%;
  height: 88rpx;
  color: white;
  line-height: 88rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-size: 32rpx;
  }
   
  .title_icon {
  position: absolute;
  height: 88rpx;
  bottom: 0;
  left: 0;
  z-index: 20;
  }
  .title_icon text{
    position: absolute;
    left: 10rpx;
  }
